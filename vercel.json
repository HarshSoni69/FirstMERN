{
    
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src":"build",
            "use":"@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/api",
            "dest": "index.js"
        },
        {
            "src":"/(.*)",
            "dest":"build/"
        }
    ]
}